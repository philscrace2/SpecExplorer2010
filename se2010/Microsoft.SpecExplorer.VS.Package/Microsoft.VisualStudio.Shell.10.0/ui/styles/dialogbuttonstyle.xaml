<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vs="clr-namespace:Microsoft.VisualStudio.PlatformUI"
                    x:Uid="ResourceDictionary_1">
  <Style x:Uid="Default_Button_Style" TargetType="{x:Type vs:DialogButton}"
         BasedOn="{StaticResource {x:Type Button}}">
    <Setter x:Uid="Setter_1" Property="FrameworkElement.MinWidth" Value="75"/>
    <Setter x:Uid="Setter_2" Property="FrameworkElement.MinHeight" Value="23"/>
    <Setter x:Uid="Setter_3" Property="FrameworkElement.Margin" Value="0,0,10,10"/>
    <Setter x:Uid="Setter_4" Property="Control.Padding" Value="10,1,10,1"/>
  </Style>
</ResourceDictionary>